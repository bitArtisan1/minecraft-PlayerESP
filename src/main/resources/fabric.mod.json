{
    "schemaVersion": 1,
    "id": "playeresp",
    "version": "${version}",
    "name": "PlayerESP",
    "description": "A client-side mod that displays player names and hitboxes through walls",
    "authors": [
        "bitArtisan1"
    ],
    "contact": {
        "homepage": "https://github.com/bitartisan1/playeresp",
        "sources": "https://github.com/bitartisan1/playeresp"
    },
    "license": "GPLv3",
    "icon": "assets/playeresp/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "com.bitartisan1.playeresp.PlayerESPClient"
        ],
        "modmenu": [
            "com.bitartisan1.playeresp.integration.ModMenuIntegration"
        ]
    },
    "mixins": [
        "playeresp.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.15.0",
        "minecraft": "~1.21.4",
        "java": ">=21"
    },
    "suggests": {
        "modmenu": "*"
    }
}
